{
  "name": "rails-challenge-movie-theater",
  "description": "Rails Challenge: Movie Theater",
  "scripts": {
    "postdeploy": "bundle exec rake db:migrate && rake db:seed"
  },
  "env": {
    "APPLICATION_ROOT_URL": {
      "required": true
    },
    "HEROKU_APP_NAME": {
      "required": true
    },
    "LANG": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    }
  },
  "formation": {
  },
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "stack": "heroku-18"
}
